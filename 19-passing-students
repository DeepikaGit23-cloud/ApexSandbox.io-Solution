public Boolean isPassed(Boolean passedExam, Boolean passedAssignments, Boolean passedProject) {
    //code here
    if(passedExam=true && passedAssignments=true && passedProject=false) {
        return true;
    }else if(passedAssignments=true && passedProject=true && passedExam=false) {
        return true;
    }else if(passedExam=true && passedProject=true && passedAssignments=false) {
        return true
    }else{
        return false
    }